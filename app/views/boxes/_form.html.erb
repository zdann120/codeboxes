<%= simple_form_for(@box) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :token %>
    <%= f.association :user %>
    <%= f.input :title %>
    <%= f.input :privacy_level %>
    <%= f.input :box_language %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
